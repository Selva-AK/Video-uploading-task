    <div class="container">
        <h1 style="color:darkblue">REGISTER AND UPLOAD YOUR FILE</h1>
        <div class="flex-cls">
            <div class="flex-col">
                <form action="/upload" method="post" enctype="multipart/form-data" onsubmit="return Validate()">
                    <% item.forEach(item=> { %>
                        <input type="<%= item.Field_type %>" name="<%= item.Field_type %>" id="<%= item.ID %>"
                            placeholder="<%= item.Placeholder %> " onkeyup="Validate(`<%= item.Field_type%>`)"
                            <%=item.Mandatory %><br><br><br>


                        <% }); %>
                            <p id="err"></p>
            </div>
            <div class="flex-col">
                <div class="drag-drop-container">
                    <div id="drag-drop-area">
                        <p id="msg">DRAG AND DROP VIDEO HERE TO UPLOAD <br>
                            (Supported file formats: .mp4, .wmv, .avi, .mkv, .flw )
                        </p>
                        <img src="../public/img/cloud.png" alt="upload" height="70px;">
                        <input type="file" name="video" id="video" accept="video/*" style="display: none;" required>
                    </div>
                </div><br><br>
                <div class="explore-btn">
                    <button type="submit">UPLOAD</button>
                </div>
            </div>
            </form>
